<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
  	<servlet-name>GWTServiceCurso</servlet-name>
  	<servlet-class>com.jornada.server.service.GWTServiceCursoImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GWTServiceCurso</servlet-name>
  	<url-pattern>/jornada/GWTServiceCurso</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>GWTServicePeriodo</servlet-name>
  	<servlet-class>com.jornada.server.service.GWTServicePeriodoImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GWTServicePeriodo</servlet-name>
  	<url-pattern>/jornada/GWTServicePeriodo</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Jornada.html</welcome-file>
  </welcome-file-list>


</web-app>
